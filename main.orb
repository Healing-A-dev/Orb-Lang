@format "sh.io";

set static file:Any = io.open("SampleText.txt",r);
set static sum:Int = 0;
func string.AdvantOfCode_Bafoonery(string:String):{
  set static n:Array = {};
  for (number in [string:gmatch("%d")]):{
    number -> n[#n+1];
  }
  return [tonumber(n[1]/{n[#n]})];
}
for (line in [file:lines()]):{
  sum + line:AdvantOfCode_Bafoonery() -> sum;
}
putln sum;