@format "sh.io";
@including:{
    "Testfile" -> Testfile
}

func testFunction(words::<@Flex>, timesToPrint::<@Int>):{
    set static timesPrinted::<@Int> = 0;
    $timesToPrint || 1 -> timesToPrint;
    if ([typeof $words] != "string"):{
        error('Type '<>$words<>" is not equal to string");
    }
    repeat:{
        putln $words;
        $timesToPrint+1 -> $timesToPrint;
    } until {
        $timesPrinted == $timesToPrint;
    }
}

timesToPrint("Hello, World!", 1);