@fmt "std.io";
@including:{
  System: "System";
}

func greet():{
  set static Name:Any = System.read("What is your name: ");
  putln "Nice name "~Name~"!";
  //putln String.length(Name);
  System.read();
  System.clear();
  putln "Hello, World";
  //putln String.gsub(Name,"Heal","Fred");
}

greet();
